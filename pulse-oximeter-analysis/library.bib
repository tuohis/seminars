month = dec,
number = {6 Suppl},
pages = {S1--4, tables of contents},
pmid = {18048890},
title = {{Takuo Aoyagi: discovery of pulse oximetry.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18048890},
volume = {105},
year = {2007}
}
