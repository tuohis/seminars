issn = {1526-7598},
journal = {Anesthesia and analgesia},
keywords = {Artifacts,Fingers,Fingers: blood supply,History, 20th Century,Humans,Light,Models, Cardiovascular,Motion,Optics and Photonics,Oximetry,Oximetry: history,Oximetry: methods,Oximetry: trends,Oxygen,Oxygen: blood,Partial Pressure,Regional Blood Flow,Reproducibility of Results,Scattering, Radiation,Signal Processing, Computer-Assisted},
